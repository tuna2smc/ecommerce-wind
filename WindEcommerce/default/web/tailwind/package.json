  "scripts": {
    "clean": "rimraf ../css/*",
    "postcss": "npx postcss tailwind-source.css -o ../css/styles.css --watch --verbose",
    "watch-dev": "run-p clean watch postcss",
    "watch-bs-dev": "run-p clean watch postcss bs",
    "watch": "NODE_ENV=development TAILWIND_COMPILE_MODE=jit",
    "build": "npx postcss tailwind-source.css -o tailwind-output.css",
    "build-dev": "NODE_ENV=development run-p clean build && cp tailwind-output.css ../css/styles.css",
    "build-prod": "NODE_ENV=production run-p clean build minify output",
    "bs": "PORT=3200 PROXY_URL='https://app.contribution.test/' browser-sync start --config ./browser-sync.config.js",
    "minify": "npx csso tailwind-output.css --output ../css/styles.css",
    "output": "echo \"\\033[0;32mDone!\\033[0m\""
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
